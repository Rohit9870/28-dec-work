package dec_28_work;

import java.util.Scanner;


public class Dec_28_work {
    
    static void function(int n){
        if(n%2==0){
            System.out.println("even");
        }
        else{
            System.out.println("odd");
        }
    }
   
    public static void main(String[] args) {
       Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        function(n);
    }
    }
    

